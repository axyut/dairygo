package components

import "github.com/axyut/dairygo/internal/types"

templ GetUser(user types.User){
    <div>
        <h1>{user.UserName}</h1>
        <h2>{user.Email}</h2>
    </div>

}

script OpenPopUp(id string){
  document.getElementById(id).style.display = "flex";
}

script ClosePopUp(id string){
  document.getElementById(id).style.display = "none";
}